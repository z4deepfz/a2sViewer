							<object class="wxStaticBoxSizer" variable="StaticBoxSizer8" member="no">
								<label>快速查询</label>
								<object class="sizeritem">
									<object class="wxChoice" name="ID_CHOICE1" variable="choice_quickQuery" member="yes">
										<handler function="OnquickQuerySelect" entry="EVT_CHOICE" />
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
									<option>1</option>
								</object>
							</object>
							<flag>wxALL|wxEXPAND</flag>
							<border>5</border>
							<option>1</option>
